page {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.container {
    box-sizing: border-box;
    background-color: #fff;
    width: 90%;
    height: 20%;
    margin-top: 30rpx;
    border-radius: 20rpx;
    box-shadow: 0 0 10px 2px #bdcee0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
}
.bankImage,
.cover {
    border-radius: 20rpx;

}